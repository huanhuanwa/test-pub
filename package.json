    "karma-webpack": "^4.0.2",
    "ts-loader": "^6.0.4",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2",
    "cz-lerna-changelog": "^2.0.3",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
